{
  "name": "@nn-lang/nn-cli",
  "description": "A new CLI generated with oclif",
  "version": "0.1.8",
  "author": "SieR-VR",
  "bin": "./bin/run.js",
  "bugs": "https://github.com/nn-lang/nn/issues",
  "dependencies": {
    "@nn-lang/nn-analyzer": "workspace:^",
    "@nn-lang/nn-codegen": "workspace:^",
    "@nn-lang/nn-language": "workspace:^",
    "@nn-lang/nn-tree-sitter": "workspace:^",
    "@nn-lang/nn-type-checker": "workspace:^",
    "@oclif/core": "^4",
    "@oclif/plugin-help": "^6",
    "@oclif/plugin-plugins": "^5",
    "tree-sitter": "^0.22.4"
  },
  "devDependencies": {
    "@eslint/compat": "^1",
    "@oclif/prettier-config": "^0.2.1",
    "@oclif/test": "^4",
    "@types/chai": "^4",
    "@types/node": "^22.15.19",
    "chai": "^4",
    "eslint": "^9",
    "eslint-config-oclif": "^6",
    "eslint-config-prettier": "^10",
    "oclif": "^4",
    "shx": "^0.4.0",
    "ts-node": "^10",
    "typescript": "^5"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "files": [
    "./bin",
    "./out",
    "./oclif.manifest.json"
  ],
  "homepage": "https://github.com/nn-lang/nn",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "out/index.js",
  "oclif": {
    "bin": "nn-cli",
    "dirname": "nn-cli",
    "commands": "./out/commands",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-plugins"
    ]
  },
  "repository": "nn-lang/nn",
  "scripts": {
    "build": "shx rm -rf out && tsc -b",
    "lint": "eslint",
    "postpack": "shx rm -f oclif.manifest.json",
    "posttest": "yarn lint",
    "prepack": "oclif manifest && oclif readme",
    "version": "oclif readme && git add README.md"
  },
  "types": "out/index.d.ts"
}
