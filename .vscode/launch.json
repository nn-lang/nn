{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Extension",
      "type": "extensionHost",
      "request": "launch",
      "args": [
        "--extensionDevelopmentPath=${workspaceFolder}/packages/nn-language-server"
      ],
      "preLaunchTask": "npm: build"
    },
    {
      "name": "Launch Program",
      "program": "yarn",
      "args": [
        "nn-cli onnx ./packages/nn-test/tests/cases/Import.nn --size A=0 --target Model"
      ],
      "request": "launch",
      "skipFiles": ["<node_internals>/**"],
      "type": "node"
    }
  ]
}
